{
  "name": "sakaui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,vue}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test:init": "vitest run",
    "prepare": "husky install",
    "lp": "eslint --fix",
    "prettier:all": "prettier . --write",
    "prettier:src": "prettier --write --log-level warn \"src/**/*.{js,ts,json,tsx,css,less,vue,html,md}\""
  },
  "dependencies": {
    "vue": "^3.4.30"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/js": "^9.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "commitlint": "^19.3.0",
    "eslint": "9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.8.0",
    "husky": "9.0.11",
    "lint-staged": "^15.2.7",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.10.0",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.2",
    "vue-tsc": "^2.0.22"
  }
}
